<h2>Show group</h2>

<ul id="show-group">

  <li>
    <strong>User:</strong>
    <%= @group.user_id %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @group.name %>
  </li>

  <li>
    <strong>Vk ID:</strong>
    <%= @group.vk_id %>
  </li>

  <li>
    <strong>Vk link:</strong>
    <%= @group.vk_link %>
